{
    "manifest_version": 3,
    "name": "QR Code URL Parser",
    "description": "Parse URLs from QR code images pasted from clipboard",
    "version": "1.1.0",
    "action": {
        "default_popup": "apps/mgqrparser.html",
        "default_icon": {
            "16": "images/icon-16.png",
            "48": "images/icon-48.png",
            "128": "images/icon-128.png"
        }
    },
    "permissions": [
        "clipboardRead",
        "contextMenus"
    ],
    "background": {
        "service_worker": "apps/background.js"
    },
    "icons": {
        "16": "images/icon-16.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    }
}
